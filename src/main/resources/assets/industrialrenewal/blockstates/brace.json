{
  "multipart": [
    {
      "when": {
        "OR": [
          {
            "north": true,
            "facing": "north|west|south|east"
          },
          {
            "facing": "north"
          }
        ]
      },
      "apply": {
        "model": "industrialrenewal:block/brace"
      }
    },
    {
      "when": {
        "OR": [
          {
            "west": true,
            "facing": "north|west|south|east"
          },
          {
            "facing": "west"
          }
        ]
      },
      "apply": {
        "model": "industrialrenewal:block/brace",
        "y": -90
      }
    },
    {
      "when": {
        "OR": [
          {
            "south": true,
            "facing": "north|west|south|east"
          },
          {
            "facing": "south"
          }
        ]
      },
      "apply": {
        "model": "industrialrenewal:block/brace",
        "y":180
      }
    },
    {
      "when": {
        "OR": [
          {
            "east": true,
            "facing": "north|west|south|east"
          },
          {
            "facing": "east"
          }
        ]
      },
      "apply": {
        "model": "industrialrenewal:block/brace",
        "y": 90
      }
    },
    {
      "when": {
        "OR": [
          {
            "north": true,
            "facing": "down_north|down_west|down_south|down_east"
          },
          {
            "facing": "down_north"
          }
        ]
      },
      "apply": {
        "model": "industrialrenewal:block/brace",
        "x": 180
      }
    },
    {
      "when": {
        "OR": [
          {
            "west": true,
            "facing": "down_north|down_west|down_south|down_east"
          },
          {
            "facing": "down_west"
          }
        ]
      },
      "apply": {
        "model": "industrialrenewal:block/brace",
        "y": -90,
        "x": 180
      }
    },
    {
      "when": {
        "OR": [
          {
            "south": true,
            "facing": "down_north|down_west|down_south|down_east"
          },
          {
            "facing": "down_south"
          }
        ]
      },
      "apply": {
        "model": "industrialrenewal:block/brace",
        "y":180,
        "x": 180
      }
    },
    {
      "when": {
        "OR": [
          {
            "east": true,
            "facing": "down_north|down_west|down_south|down_east"
          },
          {
            "facing": "down_east"
          }
        ]
      },
      "apply": {
        "model": "industrialrenewal:block/brace",
        "y": 90,
        "x": 180
      }
    }
  ]
}