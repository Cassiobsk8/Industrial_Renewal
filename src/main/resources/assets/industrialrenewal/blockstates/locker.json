{
  "multipart": [
    {
      "when": {
        "facing": "north",
        "open": "false"
      },
      "apply": {
        "model": "industrialrenewal:block/locker"
      }
    },
    {
      "when": {
        "facing": "south",
        "open": "false"
      },
      "apply": {
        "model": "industrialrenewal:block/locker",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "east",
        "open": "false"
      },
      "apply": {
        "model": "industrialrenewal:block/locker",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "west",
        "open": "false"
      },
      "apply": {
        "model": "industrialrenewal:block/locker",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "north",
        "open": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/locker_open"
      }
    },
    {
      "when": {
        "facing": "south",
        "open": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/locker_open",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "east",
        "open": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/locker_open",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "west",
        "open": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/locker_open",
        "y": 270
      }
    },
    {
      "when": {
        "down": "false"
      },
      "apply": {
        "model": "industrialrenewal:block/locker_foot"
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/locker_down"
      }
    }
  ]
}