{
  "multipart": [
    {
      "apply": {
        "model": "industrialrenewal:block/catwalk_column_steel_base"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/catwalk_column_steel_side"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/catwalk_column_steel_side",
        "y": 180
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/catwalk_column_steel_side",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/catwalk_column_steel_side",
        "y": 270
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/catwalk_column_steel_down"
      }
    },
    {
      "when": {
        "pipe": "1"
      },
      "apply": {
        "model": "industrialrenewal:block/catwalk_column_steel_down_pipe"
      }
    },
    {
      "when": {
        "pipe": "2"
      },
      "apply": {
        "model": "industrialrenewal:block/catwalk_column_steel_down_pipe",
        "y": 90
      }
    }
  ]
}