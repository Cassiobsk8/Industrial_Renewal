{
  "multipart": [
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "industrialrenewal:block/energy_level"
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "industrialrenewal:block/energy_level",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "industrialrenewal:block/energy_level",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "industrialrenewal:block/energy_level",
        "y": 270
      }
    },
    {
      "when": {
        "base": "north"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge_base",
        "x": 90,
        "y": 180
      }
    },
    {
      "when": {
        "base": "south"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge_base",
        "x": 90
      }
    },
    {
      "when": {
        "base": "east"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge_base",
        "x": 90,
        "y": -90
      }
    },
    {
      "when": {
        "base": "west"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge_base",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": {
        "base": "down"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge_base",
        "y": 180
      }
    },
    {
      "when": {
        "base": "up"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge_base",
        "x": 180
      }
    }
  ]
}