{
  "multipart": [
    {
      "when": {
        "shape": "north_south"
      },
      "apply": {
        "model": "industrialrenewal:block/rail_crossing"
      }
    },
    {
      "when": {
        "shape": "east_west"
      },
      "apply": {
        "model": "industrialrenewal:block/rail_crossing",
        "y": 90
      }
    },
    {
      "when": {
        "snow": "layer1"
      },
      "apply": {
        "model": "industrialrenewal:block/snow_layer1"
      }
    },
    {
      "when": {
        "snow": "layer2"
      },
      "apply": {
        "model": "industrialrenewal:block/snow_layer2"
      }
    }
  ]
}