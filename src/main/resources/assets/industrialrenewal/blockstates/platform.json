{
  "multipart": [
    {
      "apply": {
        "model": "industrialrenewal:block/platform_base"
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/platform_up_true"
      }
    },
    {
      "when": {
        "up": "false"
      },
      "apply": {
        "model": "industrialrenewal:block/platform_up_false"
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/platform_down_true"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/platform_side_true"
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "industrialrenewal:block/platform_side_false"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/platform_side_true",
        "y": 90
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "industrialrenewal:block/platform_side_false",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/platform_side_true",
        "y": 180
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "industrialrenewal:block/platform_side_false",
        "y": 180
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/platform_side_true",
        "y": 270
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "industrialrenewal:block/platform_side_false",
        "y": 270
      }
    }
  ]
}