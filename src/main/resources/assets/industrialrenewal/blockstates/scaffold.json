{
  "multipart": [
    {
      "apply": {
        "model": "industrialrenewal:block/scaffold_base"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/scaffold_side"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/scaffold_side",
        "y": 180
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/scaffold_side",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/scaffold_side",
        "y": 270
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/scaffold_up"
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/scaffold_down"
      }
    }
  ]
}