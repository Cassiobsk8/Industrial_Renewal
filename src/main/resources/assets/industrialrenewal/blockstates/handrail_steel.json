{
  "multipart": [
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "industrialrenewal:block/catwalk_steel_side"
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "industrialrenewal:block/catwalk_steel_side",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "industrialrenewal:block/catwalk_steel_side",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "industrialrenewal:block/catwalk_steel_side",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "north",
        "down": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/handrail_down"
      }
    },
    {
      "when": {
        "facing": "south",
        "down": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/handrail_down",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "east",
        "down": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/handrail_down",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "west",
        "down": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/handrail_down",
        "y": 270
      }
    }
  ]
}