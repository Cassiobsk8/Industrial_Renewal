{
  "multipart": [
    {
      "apply": {
        "model": "industrialrenewal:block/pillar/catwalk_steel_pillar_base"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/pillar/catwalk_steel_pillar_side",
        "uvlock": false
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/pillar/catwalk_steel_pillar_side",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/pillar/catwalk_steel_pillar_side",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/pillar/catwalk_steel_pillar_side",
        "y": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/pillar/catwalk_steel_pillar_up",
        "uvlock": false
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/pillar/catwalk_steel_pillar_down",
        "uvlock": false
      }
    }
  ]
}