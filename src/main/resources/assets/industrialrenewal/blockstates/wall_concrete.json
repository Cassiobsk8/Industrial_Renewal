{
  "multipart": [
    {
      "when": {
        "core": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/wall_concrete"
      }
    },
    {
      "when": {
        "core": "false"
      },
      "apply": {
        "model": "industrialrenewal:block/wall_concrete_connected"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/wall_concrete_side"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/wall_concrete_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/wall_concrete_side",
        "y": 180
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/wall_concrete_side",
        "y": 270
      }
    }
  ]
}