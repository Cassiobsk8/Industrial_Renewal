{
  "multipart": [
    {
      "when": {
        "gauge": "north"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge",
        "uvlock": false
      }
    },
    {
      "when": {
        "gauge": "south"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "gauge": "east"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "gauge": "west"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge",
        "y": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge_base",
        "x": 90,
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge_base",
        "x": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge_base",
        "x": 90,
        "y": -90,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge_base",
        "x": 90,
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "up"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge_base",
        "x": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "facing": "down"
      },
      "apply": {
        "model": "industrialrenewal:block/fluid_gauge_base",
        "uvlock": false
      }
    }
  ]
}